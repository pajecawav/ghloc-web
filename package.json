{
	"name": "ghloc-web",
	"private": true,
	"scripts": {
		"dev": "pnpm build:languages && next dev",
		"build": "FORCE_COLOR=1 run-s -l build:languages build:sitemap build:app",
		"build:languages": "tsx scripts/build-languages.ts",
		"build:sitemap": "tsx scripts/build-sitemap.ts",
		"build:app": "next build",
		"start": "next start",
		"lint": "next lint",
		"tsc": "tsc --noEmit",
		"format": "prettier './**/*.{js,jsx,ts,tsx,css,json,md,yml}' --write",
		"format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,json,md,yml}'",
		"ci-check": "npm-run-all format:check --parallel lint tsc",
		"prepare": "husky"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.19",
		"@heroicons/react": "^1.0.6",
		"@tanstack/react-query": "^5.64.1",
		"@vercel/analytics": "^1.4.1",
		"@vercel/og": "^0.6.4",
		"@vercel/speed-insights": "^1.1.0",
		"classnames": "^2.5.1",
		"dayjs": "^1.11.13",
		"next": "^15.1.4",
		"nprogress": "^0.2.0",
		"ofetch": "^1.4.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hot-toast": "^2.5.1",
		"tiny-server-timing": "^0.1.5"
	},
	"devDependencies": {
		"@pajecawav/prettier-config": "^1.0.0",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^22.10.6",
		"@types/nprogress": "^0.2.3",
		"@types/react": "^19.0.7",
		"autoprefixer": "^10.4.20",
		"csstype": "^3.1.3",
		"eslint": "^9.18.0",
		"eslint-config-next": "^15.1.4",
		"husky": "^9.1.7",
		"is-ci": "^4.1.0",
		"lint-staged": "^15.3.0",
		"next-statoscope": "^1.0.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.5.1",
		"prettier": "^3.4.2",
		"tailwindcss": "^3.4.17",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"webpack": "^5.97.1"
	}
}
