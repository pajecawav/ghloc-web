{
	"name": "ghloc-web",
	"private": true,
	"scripts": {
		"dev": "pnpm build:languages && next dev",
		"build": "pnpm build:languages && pnpm build:sitemap && pnpm build:app",
		"build:languages": "tsx scripts/languages.ts",
		"build:sitemap": "tsx scripts/sitemap.ts",
		"build:app": "next build",
		"start": "next start",
		"prepare": "husky install",
		"lint": "next lint",
		"tsc": "tsc --noEmit",
		"format": "prettier './**/*.{js,jsx,ts,tsx,css,json,md,yml}' --write",
		"format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,json,md,yml}'",
		"ci-check": "npm-run-all format:check --parallel lint tsc",
		"postinstall": "is-ci || yarn prepare"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.17",
		"@heroicons/react": "^1.0.6",
		"@tanstack/react-query": "^5.14.2",
		"@vercel/analytics": "^1.1.1",
		"@vercel/og": "^0.6.1",
		"classnames": "^2.3.2",
		"dayjs": "^1.11.10",
		"next": "^14.0.4",
		"nprogress": "^0.2.0",
		"ofetch": "^1.3.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hot-toast": "^2.4.1",
		"tiny-server-timing": "^0.1.5",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^18.19.3",
		"@types/nprogress": "^0.2.3",
		"@types/react": "^18.2.45",
		"autoprefixer": "^10.4.16",
		"csstype": "^3.1.3",
		"eslint": "^8.56.0",
		"eslint-config-next": "^14.0.4",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^15.2.0",
		"next-statoscope": "^1.0.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"tailwindcss": "^3.4.0",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"webpack": "^5.89.0"
	}
}
